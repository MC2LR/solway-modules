{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false,
				"targets": {
					"browsers": [
						"> 1%",
						"last 2 versions",
						"not ie <= 8"
					]
				},
				"useBuiltIns": "usage"
			}
		]
	],
	"plugins": [
		"transform-vue-jsx",
		[
			"@babel/plugin-transform-runtime",
			{
				"corejs": 3
			}
		],
		"@babel/plugin-proposal-optional-chaining",
		[
			"component",
			{
				"libraryName": "element-ui",
				"styleLibraryName": "~src/newEnergyCloud/assets/element-ui-theme/theme"
			}
		]
	]
}